{
    "name": "jan-herman/kirby-page-builder",
    "description": "Kirby page builder",
    "version": "2.1.3",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-page-builder",
    "authors": [
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "getkirby/cms": "^4.0",
        "getkirby/composer-installer": "^1.2",
        "jan-herman/kirby-barista": "^1.0"
    },
    "extra": {
        "installer-name": "page-builder"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\PageBuilder\\": "lib/"
        }
    }
}